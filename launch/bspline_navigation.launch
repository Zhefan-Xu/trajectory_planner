<launch>
	<rosparam file="$(find trajectory_planner)/cfg/bspline_navigation/bspline_navigation.yaml"/>
	<rosparam file="$(find trajectory_planner)/cfg/bspline_navigation/occupancy_map.yaml" ns="/occupancy_map"/>
	<node pkg="trajectory_planner" type="bspline_navigation_node" name="bspline_navigation_node" output="screen" />
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find trajectory_planner)/rviz/bspline_navigation.rviz"/>
</launch>